{
  "name": "@azure-tools/typespec-providerhub",
  "version": "0.46.0",
  "author": "Microsoft Corporation",
  "description": "Azure ProviderHub TypeSpec Library",
  "homepage": "https://azure.github.io/typespec-azure-pr",
  "readme": "https://github.com/Azure/typespec-azure-pr/blob/main/README.md",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Azure/typespec-azure-pr.git"
  },
  "bugs": {
    "url": "https://github.com/Azure/typespec-azure-pr/issues"
  },
  "keywords": [
    "typespec"
  ],
  "main": "./dist/src/index.js",
  "tspMain": "./dist/src/index.js",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    },
    "./testing": {
      "types": "./dist/src/testing/index.d.ts",
      "default": "./dist/src/testing/index.js"
    }
  },
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "lib/*.tsp",
    "dist/**",
    "!dist/test/**"
  ],
  "devDependencies": {
    "@types/node": "~18.11.19",
    "@typespec/compiler": "~0.60",
    "rimraf": "~6.0.1",
    "typescript": "~5.5.4"
  },
  "scripts": {
    "clean": "rimraf ./dist ./temp",
    "build": "tsc -p .",
    "test": "echo 'no tests'",
    "test:ci": "echo 'no tests'"
  }
}